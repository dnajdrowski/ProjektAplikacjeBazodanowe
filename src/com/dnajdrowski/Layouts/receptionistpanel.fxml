<VBox>

</VBox>
<HBox prefHeight="600.0" prefWidth="793.0" stylesheets="@../styles/panel.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dnajdrowski.Controllers.receptionistController">
    <VBox>
        <VBox style="-fx-border-width: 0 4 0 0; -fx-border-color: #14668A;">
            <ImageView fitHeight="163.0" fitWidth="225.0">
                <image>
                    <Image url="@../images/recepcjonista.jpg" />
                </image>
            </ImageView>
        </VBox>
        <Button id="menuButton" fx:id="btnAdd" onAction="#checkActivity" prefHeight="145.0" prefWidth="229.0" text="DODAJ WŁASCICIELA">
        </Button>
        <Button id="menuButton" fx:id="btnList" onAction="#checkActivity" prefHeight="145.0" prefWidth="229.0" text="LISTA WŁASCICIELI">
        </Button>
        <Button id="logoutButton" fx:id="btnLogout" onAction="#checkActivity" prefHeight="155.0" prefWidth="229.0" text="WYLOGUJ">
        </Button>
    </VBox>
    <AnchorPane prefHeight="600.0" prefWidth="571" stylesheets="@../styles/anchorpane.css">
        <AnchorPane fx:id="anchorList" prefHeight="600" prefWidth="571.0">
            <TableView fx:id="tableView" prefHeight="600.0" prefWidth="555">
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
                <columns>
                    <TableColumn text="Imie">
                        <cellValueFactory>
                            <PropertyValueFactory property="firstName" />
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn text="Nazwisko">
                        <cellValueFactory>
                            <PropertyValueFactory property="lastName" />
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn text="E-mail">
                        <cellValueFactory>
                            <PropertyValueFactory property="email" />
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn text="Telefon">
                        <cellValueFactory>
                            <PropertyValueFactory property="phoneNumber" />
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn text="Pesel">
                        <cellValueFactory>
                            <PropertyValueFactory property="peselNumber" />
                        </cellValueFactory>
                    </TableColumn>
                    <TableColumn text="Data urodzenia">
                        <cellValueFactory>
                            <PropertyValueFactory property="birthDate" />
                        </cellValueFactory>
                    </TableColumn>
                </columns>
            </TableView>
        </AnchorPane>
        <AnchorPane fx:id="anchorAdd" prefHeight="600.0" prefWidth="571.0">
            <children>
                <Label layoutX="30.0" layoutY="52.0" prefHeight="34.0" prefWidth="214.0" text="DANE PERSONALNE" textAlignment="CENTER">
                    <font>
                        <Font name="Times New Roman Bold" size="21.0" />
                    </font>
                </Label>
                <Label layoutX="310.0" layoutY="56.0" prefHeight="26.0" prefWidth="229.0" text="DANE KONTAKTOWE">
                    <font>
                        <Font name="Times New Roman Bold" size="21.0" />
                    </font>
                </Label>
                <Label layoutX="11.0" layoutY="117.0" text="Imię:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <Label layoutX="11.0" layoutY="186.0" text="Nazwisko:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <Label layoutX="11.0" layoutY="254.0" text="Pesel:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <Label layoutX="11.0" layoutY="320.0" text="Data urodzenia:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <Label layoutX="11.0" layoutY="384.0" text="Hasło:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <TextField fx:id="imieTextField" alignment="CENTER" layoutX="95.0" layoutY="115.0" />
                <TextField fx:id="nazwiskoTextField" alignment="CENTER" layoutX="95.0" layoutY="184.0" />
                <TextField fx:id="peselTextField" alignment="CENTER" layoutX="95.0" layoutY="252.0" />
                <DatePicker fx:id="datePicker" layoutX="136.0" layoutY="317.0" prefHeight="26.0" prefWidth="108.0" />
                <PasswordField fx:id="passwordField" alignment="CENTER" layoutX="95.0" layoutY="382.0" />
                <Label layoutX="311.0" layoutY="117.0" text="E-mail:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <Label layoutX="310.0" layoutY="187.0" text="Telefon:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <Label layoutX="309.0" layoutY="256.0" text="Miasto:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <Label layoutX="309.0" layoutY="320.0" text="Kod:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <Label layoutX="309.0" layoutY="385.0" text="Ulica:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <Label layoutX="309.0" layoutY="437.0" prefHeight="20.0" prefWidth="108.0" text="Numer domu:">
                    <font>
                        <Font name="Times New Roman Bold" size="17.0" />
                    </font>
                </Label>
                <TextField fx:id="emailTextField" alignment="CENTER" layoutX="392.0" layoutY="115.0" />
                <TextField fx:id="phoneTextField" alignment="CENTER" layoutX="392.0" layoutY="184.0" />
                <TextField fx:id="cityTextField" alignment="CENTER" layoutX="392.0" layoutY="254.0" />
                <TextField fx:id="codeTextField" alignment="CENTER" layoutX="392.0" layoutY="318.0" />
                <TextField fx:id="streetTextField" alignment="CENTER" layoutX="392.0" layoutY="383.0" />
                <TextField fx:id="numberTextField" alignment="CENTER" layoutX="467.0" layoutY="434.0" prefHeight="25.0" prefWidth="76.0" />
                <Button fx:id="btnAddUser" layoutX="425.0" layoutY="506.0" mnemonicParsing="false" onAction="#addUser" prefHeight="47.0" prefWidth="103.0" text="Dodaj">
                    <font>
                        <Font name="Times New Roman Bold" size="18.0" />
                    </font>
                </Button>
                <Button fx:id="btnClear" layoutX="21.0" layoutY="525.0" mnemonicParsing="false" onAction="#clearData" prefHeight="26.0" prefWidth="149.0" text="Wyczyść dane">
                    <font>
                        <Font name="Times New Roman Bold" size="15.0" />
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </AnchorPane>
</HBox>
